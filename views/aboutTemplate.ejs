    <link rel="stylesheet" href="/stylesheets/template.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="/stylesheets/facultyandstaff.css">
</head>

<body>
    <% include header.ejs %>
        <div id="content-body">

            <h1>
