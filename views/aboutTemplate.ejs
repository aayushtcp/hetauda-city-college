                                                <% } %>





                                                    <!-- including faculty and staff page here -->

                                                    <% if (typeof isFacultyPeople !=='undefined' ) { %>
                                                        <% include facultyandstaff.ejs %>
                                                            <% } %>



                                                                <!-- footer  -->
                                                                <% include footer.ejs %>
        </div>

        <script src="/javascripts/template.js"></script>
</body>

</html>