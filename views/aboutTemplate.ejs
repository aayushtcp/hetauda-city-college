                    <% } %>
            </div>

            <div id="description">


                <% if (typeof paragraphOne !=='undefined' ) { %>
                    <p>
                        <%= paragraphOne %>
                    </p>
                    <% } %>

                        <% if (typeof paragraphTwo !=='undefined' ) { %>w
                            <p>
                                <%= paragraphTwo %>
                            </p>
                            <% } %>


                                <% if (typeof paragraphThree !=='undefined' ) { %>
                                    <p>
                                        <%= paragraphThree %>
                                    </p>
                                    <% } %>


                                        <% if (typeof paragraphFour !=='undefined' ) { %>
                                            <p>
                                                <%= paragraphFour %>
                                            </p>
                                            <% } %>

                                                <% if (typeof paragraphFive !=='undefined' ) { %>
                                                    <p>
                                                        <%= paragraphFive %>
                                                    </p>
                                                    <% } %>
            </div>



            <!-- in this he wychscc.ejs page will render here and  -->
            <% if (typeof whyhcc !=='undefined' ) { %>
                <% include whyhcc.ejs %>
                    <%= whyhcc %>
                        <% } %>


                            <!-- addding description on whyhscc page -->


                            <% if (typeof quote !=='undefined' ) { %>
                                <p>
                                    <%= quote %>
                                </p>
                                <% } %>

                                    <% if (typeof whyhccparagraphone !=='undefined' ) { %>
                                        <p>
                                            <%= whyhccparagraphone %>
